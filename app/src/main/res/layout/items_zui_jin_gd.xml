<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="10dp"
    android:background="@color/color_white_ffffff"
    android:orientation="vertical"
    android:paddingLeft="15dp"
    android:paddingTop="10dp"
    android:paddingRight="15dp"
    android:paddingBottom="10dp">

    <TextView
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="工单编号"
        android:textColor="@color/text_content"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_faId"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="2019"
        android:textColor="@color/text_item"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv"
        app:layout_constraintRight_toLeftOf="@+id/tv0"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="工单类别描述"
        android:textColor="@color/text_content"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv_faId"
        app:layout_constraintRight_toLeftOf="@+id/tv_faTypeDesc"
        app:layout_constraintTop_toTopOf="@+id/tv"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_faTypeDesc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="30"
        android:textColor="@color/text_item"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv0"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="换表时间"
        android:textColor="@color/text_content"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_faId" />

    <TextView
        android:id="@+id/tv_faCompDttm"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="2019"
        android:textColor="@color/text_item"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv1"
        app:layout_constraintRight_toLeftOf="@+id/tv6"
        app:layout_constraintTop_toTopOf="@+id/tv1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="服务点编号"
        android:textColor="@color/text_content"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toLeftOf="@+id/tv0"
        app:layout_constraintLeft_toRightOf="@+id/tv_faCompDttm"
        app:layout_constraintRight_toLeftOf="@+id/tv_spid"
        app:layout_constraintTop_toTopOf="@+id/tv1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_spid"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="30"
        android:textColor="@color/text_item"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv6"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="旧表表号"
        android:textColor="@color/text_content"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_faCompDttm" />

    <TextView
        android:id="@+id/tv_uninstallBadgeNum"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:minEms="7"
        android:text="24235345345345"
        android:textColor="@color/text_item"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv2"
        app:layout_constraintRight_toLeftOf="@+id/tv3"
        app:layout_constraintTop_toTopOf="@+id/tv2" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="旧表拆下读数"
        android:textColor="@color/text_content"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv_uninstallBadgeNum"
        app:layout_constraintRight_toLeftOf="@+id/tv_uninstallRead"
        app:layout_constraintTop_toTopOf="@+id/tv2" />

    <TextView
        android:id="@+id/tv_uninstallRead"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="30"
        android:textColor="@color/text_item"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv3"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv3" />

    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="新表表号"
        android:textColor="@color/text_content"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_uninstallBadgeNum" />

    <TextView
        android:id="@+id/tv_newBadgeNum"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="2342342342342"
        android:textColor="@color/text_item"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv4"
        app:layout_constraintRight_toLeftOf="@+id/tv5"
        app:layout_constraintTop_toTopOf="@+id/tv4" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="新表安装读数"
        android:textColor="@color/text_content"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv_newBadgeNum"
        app:layout_constraintRight_toLeftOf="@+id/tv_installRead"
        app:layout_constraintTop_toTopOf="@+id/tv4" />

    <TextView
        android:id="@+id/tv_installRead"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="20"
        android:textColor="@color/text_item"
        android:textSize="@dimen/text_size_meduim"
        app:layout_constraintLeft_toRightOf="@+id/tv5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv5" />

</android.support.constraint.ConstraintLayout>