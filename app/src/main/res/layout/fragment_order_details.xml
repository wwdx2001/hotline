<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv1"
            style="@style/style_text_left"
            android:text="账户编号："
            app:layout_constraintLeft_toLeftOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tv_acctId"
            style="@style/style_text_right"
            android:autoLink="all"
            android:enabled="false"
            android:text="-"
            app:layout_constraintLeft_toRightOf="@+id/tv1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv1"
            tools:ignore="MissingConstraints" />

        <View
            android:id="@+id/view_navigation"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_acctId"
            app:layout_constraintLeft_toLeftOf="@+id/tv_acctId"
            app:layout_constraintRight_toRightOf="@+id/tv_acctId"
            app:layout_constraintTop_toTopOf="@+id/tv_acctId" />

        <TextView
            android:id="@+id/tv2"
            style="@style/style_text_left"
            android:text="现场活动编号："
            app:layout_constraintLeft_toLeftOf="@+id/tv1"
            app:layout_constraintTop_toBottomOf="@+id/view_navigation"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tv_faId"
            style="@style/style_text_right"
            android:text="-"
            app:layout_constraintBottom_toBottomOf="@+id/tv2"
