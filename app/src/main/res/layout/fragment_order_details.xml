        <TextView
            android:id="@+id/tv3"
            style="@style/style_text_left"
            android:text="案例编号："
            app:layout_constraintLeft_toLeftOf="@+id/tv2"
            app:layout_constraintTop_toBottomOf="@+id/tv_faId"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tv_caseId"
